<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--约束文件 要出出现的标签符合mybatis要求-->
<!--namespace 要写dao接口的路径-->
<mapper namespace="com.rt.dao.BookTypeDao">
    <select id="selectAllBookType" resultType="com.rt.pojo.BookType">
        select type,type_detail as typeDetail from booktype;
    </select>

</mapper>